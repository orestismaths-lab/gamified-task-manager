{
  "name": "gamified-task-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:lan": "next dev -H 0.0.0.0 -p 3000",
    "build": "next build",
    "build:db": "prisma generate && node scripts/pre-build-check.js && prisma migrate deploy && next build",
    "migrate": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "start": "next start",
    "start:lan": "next start -H 0.0.0.0 -p 3000",
    "lint": "next lint",
    "studio": "dotenv -e .env.local -- prisma studio",
    "studio:win": "scripts\\prisma-studio.bat"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@prisma/client": "^6.19.0",
    "bcryptjs": "^3.0.3",
    "canvas-confetti": "^1.9.3",
    "date-fns": "^3.3.1",
    "firebase": "^12.6.0",
    "framer-motion": "^11.3.0",
    "lucide-react": "^0.378.0",
    "next": "14.2.5",
    "pg": "^8.11.3",
    "prisma": "^6.19.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/canvas-confetti": "^1.6.4",
    "@types/node": "^20.14.10",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2"
  },
  "overrides": {
    "rimraf": "^5.0.0",
    "glob": "^10.0.0"
  }
